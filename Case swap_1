var swapCase = prompt("Please enter a text");

function caseSwap(swapCase){
  
  if (swapCase == swapCase.toLocaleLowerCase());
   return swapCase.toLocaleUpperCase();
  
  if (swapCase == swapCase.toLocaleUpperCase())
  return swapCase.toLocaleLowerCase();
  
  
}
console.log(caseSwap(swapCase));
